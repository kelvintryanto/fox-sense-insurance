<%- include("./../partial/_header.ejs") %>

<div class="container mt-3">
  <div class="text-end">
    <a class="nav-link" href="" id="themeToggleBtn"><i class="fas fa-sun"></i></a>
  </div>
</div>
<div class="card-container text-center mb-4">
  <div class="d-flex justify-content-center align-items-center">
    <div class="col-3">
      <h2 class="my-5">Fox Sense Insurance</h2>
      <div class="card shadow p-4">
        <h3 class="text-center mb-5">Change New Password</h3>
        <form>
          <div class="form-floating mb-3">
            <input type="email" class="form-control" id="floatingInput" placeholder="Email" autocomplete="off" />
            <label for="floatingInput">New password</label>
          </div>
          <div class="form-floating mb-3">
            <input type="password" class="form-control" id="floatingPassword" placeholder="Password" autocomplete="off" />
            <label for="floatingPassword">Repeat new Password</label>
          </div>
          <button class="btn btn-primary w-100 mb-3">Submit</button>
        </form>
      </div>
    </div>
  </div>
</div>

<%- include("./../partial/_footer.ejs") %>
